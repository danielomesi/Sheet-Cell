<?xml version="1.0" encoding="UTF-8"?>
<STL-Sheet name="test-sheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="shtisel-v1.xsd">
    <STL-Layout rows="4" columns="6">
        <STL-Size column-width-units="15" rows-height-units="3"/>
    </STL-Layout>
    
    <STL-Cells>
        <!-- Simple Values -->
        <STL-Cell row="1" column="A">
            <STL-Original-Value>-5</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="1" column="B">
            <STL-Original-Value>10.5</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="1" column="C">
            <STL-Original-Value>true</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="1" column="D">
            <STL-Original-Value>Hello World</STL-Original-Value>
        </STL-Cell>

        <!-- PLUS Function -->
        <STL-Cell row="2" column="A">
            <STL-Original-Value>{PLUS,5,10}</STL-Original-Value>
        </STL-Cell>

        <!-- MINUS Function -->
        <STL-Cell row="2" column="B">
            <STL-Original-Value>{MINUS,10,5}</STL-Original-Value>
        </STL-Cell>

        <!-- TIMES Function -->
        <STL-Cell row="2" column="C">
            <STL-Original-Value>{TIMES,3,4}</STL-Original-Value>
        </STL-Cell>

        <!-- DIVIDE Function (Edge Case: Division by zero) -->
        <STL-Cell row="2" column="D">
            <STL-Original-Value>{DIVIDE,10,0}</STL-Original-Value>
        </STL-Cell>

        <!-- MOD Function -->
        <STL-Cell row="3" column="A">
            <STL-Original-Value>{MOD,10,3}</STL-Original-Value>
        </STL-Cell>

        <!-- POW Function -->
        <STL-Cell row="3" column="B">
            <STL-Original-Value>{POW,2,3}</STL-Original-Value>
        </STL-Cell>

        <!-- ABS Function (Negative Input) -->
        <STL-Cell row="3" column="C">
            <STL-Original-Value>{ABS,-5}</STL-Original-Value>
        </STL-Cell>

        <!-- CONCAT Function -->
        <STL-Cell row="3" column="D">
            <STL-Original-Value>{CONCAT,Hello, World}</STL-Original-Value>
        </STL-Cell>

        <!-- SUB Function (Edge Case: Valid Substring) -->
        <STL-Cell row="4" column="A">
            <STL-Original-Value>{SUB,HelloWorld,0,5}</STL-Original-Value>
        </STL-Cell>

        <!-- SUB Function (Edge Case: Start/End Index Out of Bounds) -->
        <STL-Cell row="4" column="B">
            <STL-Original-Value>{SUB,Hello,1,10}</STL-Original-Value>
        </STL-Cell>
        
        <!-- Nested Function (Edge Case: Complex Function Nesting) -->
        <STL-Cell row="4" column="C">
            <STL-Original-Value>{PLUS,{TIMES,2,3},{DIVIDE,10,2}}</STL-Original-Value>
        </STL-Cell>
        
    </STL-Cells>
</STL-Sheet>
