<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fx:id="wrapper" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.scenes.dashboard.sheetsTable.SheetsTableController">
   <content>
      <VBox alignment="CENTER" spacing="10.0">
         <children>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="requestReadAccessButton" disable="true" mnemonicParsing="false" onAction="#requestReadAccessButtonClicked" text="Request Read Access" wrapText="true" />
                  <Button fx:id="requestWriteAccessButton" disable="true" mnemonicParsing="false" onAction="#requestWriteAccessButtonClicked" text="Request Write Access" wrapText="true" />
                  <Button fx:id="viewSheetButton" disable="true" mnemonicParsing="false" onAction="#viewSheetButtonClicked" text="View Sheet" wrapText="true" />
                  <Label fx:id="statusLabel" wrapText="true" />
               </children>
            </HBox>
            <Separator />
            <TableView fx:id="tableView" maxHeight="300.0" maxWidth="1.7976931348623157E308">
              <columns>
                <TableColumn minWidth="70.0" prefWidth="-1.0" text="Uploader" />
                <TableColumn minWidth="80.0" prefWidth="-1.0" text="Sheet Name" />
                  <TableColumn minWidth="80.0" prefWidth="-1.0" text="Sheet Size" />
                  <TableColumn minWidth="80.0" prefWidth="-1.0" text="Access Level" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </content>
</ScrollPane>
